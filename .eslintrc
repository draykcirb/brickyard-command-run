{
	//"parser": "babel-eslint",
	"extends": "airbnb-base",
	"rules": {
		"comma-dangle": [1, "only-multiline"],
		"semi": [2, "never"],
		"prefer-const": 0,
		"camelcase": 1,
		"indent": [2, "tab", {"SwitchCase": 1}],
		"object-shorthand": 0,
		"global-require": 1,
		"func-names": 0,
		"consistent-return": 1,
		"max-len": [1, 120, 2],
		"no-else-return": 1,
		"no-use-before-define": ["error", { "functions": false, "classes": true }],
		"no-labels": 1,
		"no-param-reassign": [2, { "props": false }],
		"no-loop-func": 1,
		"prefer-arrow-callback": 1,
		"strict": 0
	}
}
